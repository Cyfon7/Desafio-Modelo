<p id="notice"><%= notice %></p>

<h1>Posts</h1>
<div id="show"></div>

<%= render 'posts/form', post: @post %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="myPost">
    <%= render @posts %>
  </tbody>
</table>

